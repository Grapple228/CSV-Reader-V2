<?xml version="1.0" encoding="utf-8"?>
<settings>
  <global>
    <IsTracingEnabled>false</IsTracingEnabled>
    <defaultFileName>New File</defaultFileName>
    <defaultColumnName>Column</defaultColumnName>
    <showStatusBar>false</showStatusBar>
    <defaultFileExtension>csv</defaultFileExtension>
    <minimumWindowWidth>800</minimumWindowWidth>
    <minimumWindowHeight>600</minimumWindowHeight>
  </global>
  <local>
    <csv>
      <defaultColumnCount>1</defaultColumnCount>
      <defaultSeparator>;</defaultSeparator>
      <supportedSeparators>;,.</supportedSeparators>
      <separatorColor>#FF0000</separatorColor>
      <showAsTable>true</showAsTable>
      <stretchCells>true</stretchCells>
      <hideEmptyRows>false</hideEmptyRows>
    </csv>
    <SQLite />
    <MySQL />
    <PostgreSQL />
  </local>
</settings>